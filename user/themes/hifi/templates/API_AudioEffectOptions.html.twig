    <table class="props">
      <thead>
        <tr>
          <th>Name</th>
          <th>Type</th>
          <th>Default</th>
          <th class="last">Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td class="name"><code>bandwidth</code></td>
          <td class="type"> <span class="param-type">number</span></td>
          <td class="default"> 10000</td>
          <td class="description last">The corner frequency (Hz) of the low-pass filter at reverb input.</td>
        </tr>
        <tr>
          <td class="name"><code>preDelay</code></td>
          <td class="type"> <span class="param-type">number</span></td>
          <td class="default"> 20</td>
          <td class="description last">The delay (milliseconds) between dry signal and the onset of early reflections.</td>
        </tr>
        <tr>
          <td class="name"><code>lateDelay</code></td>
          <td class="type"> <span class="param-type">number</span></td>
          <td class="default"> 0</td>
